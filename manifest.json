{
    "manifest_version": 3,
    "name": "INHA Proxy Redirect",
    "version": "1.0",
    "description": "Redirects to the current page with INHA proxy",
    "permissions": ["tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  